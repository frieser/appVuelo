{% extends 'AgoratecVueloAppBundle::layout.html.twig' %}

{% block title %}Formulario de acceso{% endblock %}

{% block id 'usuario' %}


{% block body %}
	{{ parent() }}

	{% include "AgoratecVueloAppBundle:Navigation:navbar.html.twig"%}

	{% include "AgoratecVueloAppBundle:Home:intro.html.twig"%}

	{% if error %}
		<div>{{ error.message }}</div>
	{% endif %}
	
	<form action="{{ path('usuario_login_check') }}" method="post">
		<label for="username">Usuario:</label>
		<input type="text" id="username" name="_username"
		value="{{ last_username }}" />
		<label for="password">Contrase√±a:</label>
		<input type="password" id="password" name="_password" />
		<input type="submit" name="login" value="Acceder" />
	</form>

{% endblock %}


	

{% block aside %}{% endblock %}
